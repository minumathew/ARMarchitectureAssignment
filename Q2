/*	Given three number find the largest of the three using Cortex M4 */
  AREA     appcode, CODE, READONLY
		
		ENTRY 
		EXPORT __main
__main  		 

    MOV R0, #10
		MOV R1, #11
		MOV R2, #12
		CMP R0,R1
		               
    ITE GT
		MOVGT R3,R0
		MOVLE R3,R1
		
		CMP R3,R2
		ITE GT
		MOVGT R3,R3
		MOVLE R3,R2
    END
